unit GateXml;

interface

uses
  Vcl.Forms, Xml.XMLDoc;

type
  TGateXmlParser = class(TObject)
  private
    doc: TXmlDocument;
  public
    constructor Create;
    destructor Destroy; override;
  end;

  TGateXmlNode = class(TObject)
  end;
var
  xmlForm: TForm;

implementation

constructor TGateXmlParser.Create;
begin
  inherited;
  doc:=TXmlDocument.Create(xmlForm);
end;

destructor TGateXmlParser.Destroy;
begin
  FreeAndNil(doc);
  inherited;
  // TODO -cMM: TGateXmlParser.Destroy default body inserted
end;

end.
